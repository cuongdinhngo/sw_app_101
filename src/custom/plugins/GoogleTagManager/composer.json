{
    "name": "google-tag-manager/google-tag-manager",
    "description": "google-tag-manager/google-tag-manager",
    "type": "shopware-platform-plugin",
    "version": "1.0.0",
    "license": "MIT",
    "require": {
        "shopware/core": "~6.6.0"
    },
    "extra": {
        "shopware-plugin-class": "GoogleTagManager\\GoogleTagManager",
        "label": {
            "de-DE": "Google Tag Manager",
            "en-GB": "Google Tag Manager"
        },
        "plugin-icon": "src/Resources/public/google.png"
    },
    "autoload": {
        "psr-4": {
